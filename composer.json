{
  "name": "lotgd/module-gender",
  "description": "Prompts the user with a selection for a gender at a new day.",
  "type": "lotgd-module",
  "license": "AGPL-3.0",
  "version": "v0.6.0",
  "authors": [
    {
      "name": "Basilius Sauter",
      "email": "basilius.sauter@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "LotGD\\Module\\Gender\\": "src/"
    }
  },
  "repositories": [
    {"type": "path", "url": "../core"},
    {"type": "path", "url": "../module-new-day"},
    {"type": "path", "url": "../module-village"},
    {
      "type": "composer",
      "url": "https://raw.githubusercontent.com/lotgd/packages/master/build/packages.json"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable" : true,

  "require": {
    "lotgd/core": ">=0.6",
    "lotgd/module-village": ">=0.5.0-alpha",
    "lotgd/module-new-day": ">=0.5.0-alpha"
  },

  "require-dev": {
    "phpunit/phpunit": "*",
    "symfony/yaml": "*"
  },

  "config": {
    "secure-http": false
  }
}
