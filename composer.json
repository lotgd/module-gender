{
  "name": "lotgd/module-gender",
  "description": "Prompts the user with a selection for a gender at a new day.",
  "type": "lotgd-module",
  "license": "AGPL-3.0-or-later",
  "authors": [
    {
      "name": "Basilius Sauter",
      "email": "basilius.sauter@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "LotGD\\Module\\Gender\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://raw.githubusercontent.com/lotgd/packages/master/build/packages.json"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable" : true,

  "require": {
    "lotgd/core": "^0.6.0",
    "lotgd/module-village": "^0.6",
    "lotgd/module-new-day": "^0.6"
  },

  "require-dev": {
    "phpunit/phpunit": "*",
    "symfony/yaml": "*"
  },

  "config": {
    "secure-http": false
  }
}
